<nav class="navbar">
    <div class="container-fluid">
        <div class="navbar-brand" href="#">Quizzie-Moto</div>
    
        <div class="nav navbar-nav navbar-right">
            <form class="form-inline">
            
            <!-- Trigger teacher registration modal -->
            <button type="button" class="btn btn-info btn-md" data-toggle="modal" data-target="#teacherRegister" data-whatever="register">Sign Up</button>
                
            <!-- Trigger login modal -->
            <button type="button" class="btn btn-info btn-md" data-toggle="modal" data-target="#teacherLogin" data-whatever="login">Login</button>
            </form>
        
        </div>
